{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "properties": {
    "location": {
      "type": "object",
      "properties": {
        "alt": {
          "type": "object",
          "properties": {
            "measurement": {
              "type": "string"
            },
            "value": {
              "type": "number"
            }
          }
        },
        "lat": {
          "type": "number"
        },
        "lon": {
          "type": "number"
        }
      }
    },
    "reading": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "measurement": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "sensorType": {
      "type": "string"
    },
    "time": {
      "type": "string"
    },
    "tower": {
      "type": "string"
    }
  }
}
